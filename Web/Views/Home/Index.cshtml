@model IEnumerable<Domain.PetType> 

<h1>Pet Types</h1>

<table class="table">
    <thead>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var petType in Model)
    {
        <tr>
            <td>@petType.Id</td>
            <td>@petType.Name</td>
            @* <td>@petType.Description</td> *@
        </tr>
    }
    </tbody>
</table>